<section class="multiple-product-cart">
    <div class="container">
        <div class="box-steps d-flex">
            <div class="steps active">
                <div class="rounded-50">1</div>
                <h6>Cart</h6>
            </div>
            <div class="steps">
                <div class="rounded-50">2</div>
                <h6>Delivery Address</h6>
            </div>
            <div class="steps">
                <div class="rounded-50">3</div>
                <h6>Billing Information</h6>
            </div>
            <div class="steps">
                <div class="rounded-50">4</div>
                <h6>Payment</h6>
            </div>
        </div>

        <div class="box-shopping-cart">
            <div class="row parents header">
                <div class="col-md-9">
